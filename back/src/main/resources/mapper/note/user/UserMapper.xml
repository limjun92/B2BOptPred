<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.note.user.UserMapper">

	<select id="selectUserList" resultType="User">
		select * from users
	</select>
	
	<insert id="signUp" parameterType="User">
        insert into users (usersId,usersPassword,usersTel,usersNickname,
        usersName,usersEmail)
        values (#{usersId}, #{usersPassword}, #{usersTel},
         #{usersNickname},  #{usersName}, #{usersEmail})
     </insert>
	
</mapper>